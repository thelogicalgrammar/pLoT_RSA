   "source": [
    "S = z3.DeclareSort('S')\n",
    "f = z3.Function('f', S, S)\n",
    "x = z3.Const('x', S)\n",
    "z3.solve(f(f(x)) == x, f(f(f(x))) == x)\n",
    "z3.solve(f(f(x)) == x, f(f(f(x))) == x, f(x) != x)"
   ]
